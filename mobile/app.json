{
  "expo": {
    "name": "Break Bread.",
    "description": "Organize dinners with family and friends. Have fun.",
    "slug": "break-bread",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": ["ios"],
    "version": "1.0.0",
    "githubUrl": "https://github.com/blakeface/break-bread",
    "orientation": "portrait",
    "icon": "./assets/icon.png", // should be 1024x1024 png file
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
